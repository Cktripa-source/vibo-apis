{
  "name": "affiliate-marketplace-backend",
  "version": "1.0.0",
  "description": "Multi-role marketplace backend with affiliate tracking",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
     "dev": "nodemon --watch \"src/**/*.ts\" --exec tsx src/server.ts",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "migrate": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "seed": "tsx prisma/seed.ts",
    "test": "vitest",
    "test:e2e": "vitest run tests/*.e2e.test.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "affiliate",
    "marketplace",
    "nodejs",
    "express",
    "typescript",
    "prisma"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.12.0",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.18.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.7",
    "nodemailer": "^6.9.8",
    "stripe": "^14.9.0",
    "winston": "^3.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.12",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.1",
    "prisma": "^5.17.0",
    "supertest": "^7.0.0",
    "tsx": "^4.9.3",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
